{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "builds": [
    {
        "src": "apps/server/main.js",
        "use": "@vercel/node"
    }
  ],
  "routes": [{
    "src": "/(.*)",
    "dest": "apps/server/main.js",
    "methods": [
      "GET",
      "POST",
      "PUT",
      "DELETE"
  ]
  }]
}